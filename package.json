{
  "name": "reisetopia-plugin",
  "private": true,
  "scripts": {
    "build": "npm run production",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development npm-run-all --parallel webpack:*",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production npm-run-all --parallel webpack:*",
    "update:interactive": "npx npm-check -uE",
    "watch": "cross-env NODE_ENV=development npm-run-all --parallel 'webpack:* -- --watch'",
    "webpack:reisetopia-plugin": "webpack --stats-children --config webpack.reisetopia-plugin.js",
    "eslint": "eslint 'src/**/*.js'",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint 'src/**/*.{css,scss,vue}'",
    "stylelint:fix": "npm run stylelint -- --fix",
    "build:plugin": "npm run production && npm run package",
    "package": "./package.sh"

  },
  "browserslist": [
    "last 2 versions",
    "not IE 11",
    "not dead"
  ],
  "dependencies": {
    "@popperjs/core": "2.11.7",
    "bootstrap": "5.2.3",
    "core-js": "^3.26.0",
    "jquery": "^3.4.1",
    "prismjs": "^1.23.0",
    "sass": "1.62.1"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/eslint-parser": "7.21.8",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.21.5",
    "autoprefixer": "10.4.14",
    "babel-loader": "9.1.2",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "5.0.0",
    "eslint": "8.39.0",
    "eslint-plugin-jsdoc": "43.1.1",
    "eslint-plugin-unicorn": "46.0.0",
    "file-loader": "^6.2.0",
    "lint-staged": "^13.1.0",
    "mini-css-extract-plugin": "2.7.5",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.41",
    "postcss-import": "15.1.0",
    "postcss-inline-svg": "6.0.0",
    "postcss-loader": "7.3.0",
    "postcss-mixins": "9.0.4",
    "postcss-nested": "6.0.1",
    "postcss-nested-ancestors": "^3.0.0",
    "postcss-preset-env": "8.3.2",
    "postcss-pxtorem": "6.0.0",
    "postcss-scss": "^4.0.9",
    "postcss-sort-media-queries": "5.1.0",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "13.2.2",
    "style-loader": "^3.3.3",
    "webpack": "5.81.0",
    "webpack-cli": "5.0.2",
    "webpack-manifest-plugin": "5.0.0",
    "webpack-merge": "5.8.0"
  }
}
